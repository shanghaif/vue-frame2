  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-port": "cross-env PORT=7060 npm run dev",
    "dev": "npm run createProxySelf && npm run lint && npm run serve",
    "dev:test": "npm run createProxySelf && npm run lint && vue-cli-service serve --mode test",
    "serve": "npm run createProxySelf && vue-cli-service serve",
    "serve:dev-preview": "npm run build:theme-start && npm run createProxySelf && npm run lint && vue-cli-service serve --mode preview",
    "build": "npm run build:theme-start && vue-cli-service build",
    "build:test": "vue-cli-service build --mode testProd",
    "lint": "eslint --ext .js --ext .jsx --ext .vue src/",
    "lint:no-fix": "vue-cli-service lint --no-fix",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue src/",
    "clear": "rimraf node_modules && cnpm i",
    "commitmsg": "validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "icon": "node ./build/read-icon.js",
    "createProxySelf": "node ./build/create-proxy-self.js",
    "build:theme-start": "gulp themeClear --default,light,black && npm run build:theme && npm run build:theme-inject",
    "build:theme": "gulp theme --default,light,black",
    "build:theme-inject": "gulp themeInjectHtml",
    "build:theme-light": "gulp theme --light"
  },
